        "sensiolabs/ansi-to-html": "^1.1",
        "simplepie/simplepie": "^1.3",
        "symfony/monolog-bundle": "^2.8 || ^3.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "^3.3.7",
        "tecnickcom/tcpdf": "^6.2.22",
