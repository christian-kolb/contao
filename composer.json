{
    "name": "contao/contao",
    "type": "symfony-bundle",
    "description": "Contao 4 bundles",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-dom": "*",
